{{> header}}
<div class="col-md-6 text-center col-lg">
    <img class="profile-img" src="./images/user.png" />
    <div class="profile-info">
      <p>Username: <small>{{ username }}</small></p>
      <p class="infoType">Organizer of {{ events.length }} events.</p>

      {{#if thereIsEvents}}

      {{#each events}}
      <p>{{this.name}}</p>     
      
      {{else}}
      <p>No events</p>
      {{/each}}
      
      {{/if}}      
    </div>
  </div>

  {{> footer}}